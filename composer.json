{
  "name": "matthiasmullie/scrapbook",
  "type": "library",
  "description": "Scrapbook is a caching environment for PHP, with adapters for e.g. Memcached, Redis, Couchbase, APC, SQL and additional capabilities (e.g. transactions, stampede protection) built on top.",
  "keywords": [
    "cache", "caching",
    "key", "value", "store",
    "apc",
    "memcached",
    "redis",
    "couchbase",
    "sql", "mysql", "sqlite", "postgresql",
    "filesystem", "flysystem",
    "buffer", "buffered",
    "transaction", "transactional", "commit", "rollback",
    "stampede", "protection", "mitigation",
    "psr6", "psr-6", "psr-cache"
  ],
  "homepage": "http://scrapbook.cash",
  "license": "MIT",
  "authors": [
    {
      "name": "Matthias Mullie",
      "homepage": "http://www.mullie.eu",
      "email": "scrapbook@mullie.eu",
      "role": "Developer"
    }
  ],
  "require": {
    "php": "^5.3|^7.0",
    "psr/cache": "~1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8",
    "satooshi/php-coveralls": "~1.0",
    "cache/integration-tests": "dev-master"
  },
  "suggest": {
    "ext-apc": ">=3.1.1",
    "ext-couchbase": ">=2.0.0",
    "ext-memcached": ">=2.0.0",
    "ext-redis": ">=2.2.0 || 0.0.0.0",
    "ext-pdo": ">=0.1.0",
    "league/flysystem": "~1.0"
  },
  "provide": {
    "psr/cache-implementation": "~1.0"
  },
  "autoload": {
    "psr-4": {
      "MatthiasMullie\\Scrapbook\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MatthiasMullie\\Scrapbook\\Tests\\": "tests/"
    }
  }
}
